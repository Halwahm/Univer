<UserControl x:Class="lab4_5.UndoRedo"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:lab4_5"
             mc:Ignorable="d">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width = "1*" />
            <ColumnDefinition Width="1*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="Undo" Style="{DynamicResource DefaultButton}" Content="Undo" Cursor="Hand" Grid.Column="0" VerticalAlignment="Center" HorizontalAlignment="Center" Click="Undo_Click"/>
        <Button x:Name="Redo" Style="{DynamicResource DefaultButton}" Content="Redo" Cursor="Hand" Grid.Column="1" VerticalAlignment="Center" HorizontalAlignment="Center" Click="Redo_Click"/>
    </Grid>
</UserControl>
