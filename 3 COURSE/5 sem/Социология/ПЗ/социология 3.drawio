        <mxCell id="UKYrtE4_NKRgBVxxTzd2-28" value="&lt;font style=&quot;font-size: 17px;&quot;&gt;Точечное&lt;/font&gt;" style="rounded=1;whiteSpace=wrap;html=1;" vertex="1" parent="1">
